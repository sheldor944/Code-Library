{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Code",
      "program": "c:/Code/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "sourceLanguages": [
        "swift"
      ],
      "args": [],
      "cwd": "${workspaceFolder:Code}",
      "name": "Debug Code",
      "program": "${workspaceFolder:Code}\\.build\\debug\\Code.exe",
      "preLaunchTask": "swift: Build Debug Code"
    },
    {
      "type": "lldb",
      "request": "launch",
      "sourceLanguages": [
        "swift"
      ],
      "args": [],
      "cwd": "${workspaceFolder:Code}",
      "name": "Release Code",
      "program": "${workspaceFolder:Code}\\.build\\release\\Code.exe",
      "preLaunchTask": "swift: Build Release Code"
    }
  ]
}